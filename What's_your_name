def print_full_name(first,last):
    
    original_string = 'Hello firstname lastname! You just delved into python.'
    
    modified_string = original_string.replace('firstname', first_name).replace('lastname', last_name)

    print(modified_string)

if __name__ == '__main__':
    first_name = input()
